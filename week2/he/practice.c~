#include <stdio.h>
#include <cs50.h>
#include <string.h>
#include <ctype.h>

int
main(int argc, char *argv[])
{
	if(argc > 2)
	{
		printf("Error \n");
		return 1;
	}
	FILE *fp;
	string str1[100000] = {};
	char ch, str;

/* opening file for reading */
	fp = fopen("/usr/share/dict/words" , "r");
	if(fp == NULL) 
	{
		perror("Error opening file");
		return(-1);
	}
	int i = 0, j = 0;
	while((fgetc(fp) ) != EOF)
	{
		//str1[j] = str;		
		
		printf("%c", str);
		j++;		
	}
	printf("%d \n", j);
	printf("%s \n", argv[1]);
	for(i = 99100; i < j; i++)
		printf("%s", str1[i]);
	if(i == 0)
		printf("NO");
	printf("\n");
	fclose(fp);
	return(0);
}
